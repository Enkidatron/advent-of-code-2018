module Day08 exposing (Header, Node(..), NodeInfo, State, Tree(..), calculateNodeValue, calculateTreeValue, headerParser, initialState, inputNumbers, inputText, iterateState, metadataListParser, metadataListParserHelper, nodeFromNumbers, part1Answer, part1AnswerAlt, part1Test, part2, part2Answer, part2AnswerAlt, part2Test, sumTreeMetadata, testInput, testNumbers, treeListParser, treeListParserHelper, treeParser)

import Dict exposing (Dict)
import Parser exposing ((|.), (|=), Parser, Step(..))


type alias NodeInfo =
    { childCount : Int
    , metadataCount : Int
    }


type alias State =
    { nodeInfoStack : List NodeInfo
    , metadataSum : Int
    , numbers : List Int
    }


iterateState : State -> State
iterateState state =
    let
        maybeNextState =
            case state.nodeInfoStack of
                [] ->
                    case state.numbers of
                        x :: y :: rest ->
                            Just
                                { nodeInfoStack = [ NodeInfo x y ]
                                , metadataSum = state.metadataSum
                                , numbers = rest
                                }

                        x :: rest ->
                            Debug.todo "this did not work out"

                        [] ->
                            Nothing

                nodeInfo :: restOfStack ->
                    case ( nodeInfo.childCount, nodeInfo.metadataCount ) of
                        ( 0, metadataCount ) ->
                            Just
                                { nodeInfoStack = restOfStack
                                , metadataSum = state.metadataSum + List.sum (List.take metadataCount state.numbers)
                                , numbers = List.drop metadataCount state.numbers
                                }

                        ( childCount, metadataCount ) ->
                            case state.numbers of
                                x :: y :: restOfNumbers ->
                                    Just
                                        { nodeInfoStack =
                                            { childCount = x, metadataCount = y }
                                                :: ({ childCount = childCount - 1, metadataCount = metadataCount } :: restOfStack)
                                        , metadataSum = state.metadataSum
                                        , numbers = restOfNumbers
                                        }

                                _ ->
                                    Debug.todo "oops"
    in
    case maybeNextState of
        Nothing ->
            state

        Just nextState ->
            iterateState nextState


initialState numbers =
    { nodeInfoStack = []
    , metadataSum = 0
    , numbers = numbers
    }


part1Answer =
    inputNumbers |> initialState |> iterateState |> .metadataSum


part1Test =
    testNumbers |> initialState |> iterateState |> .metadataSum


type Node
    = Node
        { children : Dict Int Node
        , metadata : List Int
        }


nodeFromNumbers : List Int -> ( Node, List Int )
nodeFromNumbers numbers =
    case numbers of
        childCount :: metadataCount :: restOfNumbers ->
            let
                childIndexes =
                    List.range 1 childCount

                ( children, remainingNumbers ) =
                    List.foldl
                        (\index ( childListSoFar, numbersSoFar ) ->
                            let
                                ( childNode, nextNumbers ) =
                                    nodeFromNumbers numbersSoFar
                            in
                            ( ( index, childNode ) :: childListSoFar, nextNumbers )
                        )
                        ( [], restOfNumbers )
                        childIndexes

                metadata =
                    List.take metadataCount remainingNumbers

                finalNumbers =
                    List.drop metadataCount remainingNumbers
            in
            ( Node { children = Dict.fromList children, metadata = metadata }
            , finalNumbers
            )

        _ ->
            Debug.todo "woops"


calculateNodeValue : Node -> Int
calculateNodeValue (Node node) =
    if Dict.isEmpty node.children then
        List.sum node.metadata

    else
        List.filterMap (\metadata -> Dict.get metadata node.children) node.metadata
            |> List.map calculateNodeValue
            |> List.sum


part2 numbers =
    nodeFromNumbers numbers |> Tuple.first |> calculateNodeValue


part2Answer =
    part2 inputNumbers


part2Test =
    part2 testNumbers



